#python
a=[]
for i in range(0,6,1):
    num=int(input("숫자를 입력하세요:"))
    a.append(num)

for i in range(0,5,1):
    for j in range(0,4-i+1,1):
        if a[j]>a[j+1]:
            temp=a[j]
            a[j]=a[j+1]
            a[j+1]=temp
            print("버블(거품)정렬 과정:",a)
for k in range(0,6,1):
    print(a[k],end=" ")
    
