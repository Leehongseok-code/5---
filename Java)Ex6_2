class Point
{
	private int x,y;
	public Point(int x,int y)
	{
		this.x=x;
		this.y=y;
	}
	public String toString()
	{
		return "("+this.x+","+this.y+")";
	}
}
public class Ex6_2 {

	public static void main(String[] args) 
	{
		// TODO Auto-generated method stub
		Point p=new Point(10,50);
		System.out.println(p.toString());
	}

}
